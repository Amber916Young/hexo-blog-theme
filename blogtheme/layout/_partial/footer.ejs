  <!-- ======= Footer ======= -->
  <footer id="footer">
    <div class="container">
      <h3>Yejia Yang</h3>
      <p></p>
      <div class="social-links">
        <% for (var i in theme.socialMedia){ %>
          <a href="<%- url_for(theme.socialMedia[i].url) %>" class="<%- i %>"><i class="<%- theme.socialMedia[i].icon %>"></i></a>
        <% } %>
      </div>
      <div class="copyright">
        &copy; Copyright 2019-2023 <strong><span>YYJ</span></strong>. All Rights Reserved
      </div>
    </div>
    <% if (theme.website_count.busuanzi_count.enable === true) { %>
      <div class="website-count info-item">
          <% if (theme.website_count.busuanzi_count.site_uv) { %>
              <span id="busuanzi_container_site_uv" class="busuanzi_container_site_uv">
                  <%- __('TotalView') %>&nbsp;<span id="busuanzi_value_site_uv" class="busuanzi_value_site_uv"></span>
              </span>|
          <% } %> 
          <% if (theme.website_count.busuanzi_count.site_pv) { %>
              <span id="busuanzi_container_site_pv" class="busuanzi_container_site_pv">
                  <%- __('Visitor Count') %>&nbsp;<span id="busuanzi_value_site_pv" class="busuanzi_value_site_pv"></span>
              </span>
          <% } %>
      </div>
    <% } %>


    <div class="theme-info info-item">
      <span class="powered-by-container">
        Powered by <a class="theme-version" target="_blank" href="https://github.com/EvanNotFound/hexo-theme-redefine">Hexo</a>
      </span>
    </div>
  </footer>


  <div id="preloader"></div>
  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>
